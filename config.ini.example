# SQLite 数据库 AI 分析工具配置文件
# 将此文件重命名为 config.ini 并填写你的配置

[settings]
# AI 提供商: ollama 或 doubao
provider = doubao

[doubao]
# 豆包 AI API 密钥
api_key = 在这里填写你的豆包_api_key

# 模型选择:
# - doubao-pro-32k (推荐，32K 上下文)
# - doubao-pro-4k (快速，4K 上下文)
# - doubao-lite-4k (低成本，4K 上下文)
model = doubao-pro-32k

# API 端点地址 (一般不需要修改)
endpoint = https://ark.cn-beijing.volces.com/api/v3

[ollama]
# Ollama 服务地址 (一般不需要修改)
url = http://localhost:11434

# Ollama 模型名称
model = llama2
